  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "start:dev": "NODE_ENV=development yarn run start",
    "start:prod": "NODE_ENV=development yarn run start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint .",
    "test": "jest --watchAll"
