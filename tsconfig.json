{
  "compilerOptions": {
    "jsx": "react-native",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@assets/*": ["./src/assets/*"],
      "@store/*": ["./src/store/*"],
      "@config/*": ["./src/config/*"],
      "@api/*": ["./src/api/*"],
      "@pages/*": ["./src/pages/*"],
      "@dtos/*": ["./src/dtos/*"],
      "@helpers/*": ["./src/helpers/*"],
      "@contexts/*": ["./src/context/*"],
      "@app/*": ["./src/app/*"]
    }
  },
  "extends": "expo/tsconfig.base",
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "index.tsx",
    "/App/**/*.ts",
    "App.tsx",
    "nativewind.d.ts",
    "App/**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "declarations.d.ts", "src/api/listUrl.ts",
  ],
  "exclude": [
    "node_modules"
  ]
}
